{
    "class": "ADC",
    "schemaVersion": "3.9.0",
    "id": "Consul_SD",
        "controls": {
        "class": "Controls",
        "trace": true,
        "logLevel": "debug"
    },
    "BUBBA": {
      "class": "Tenant",
      "Nginx": {
        "class": "Application",
        "template": "http",
        "serviceMain": {
          "class": "Service_HTTP",
          "virtualPort": 8080,
          "virtualAddresses": [
            "150.20.20.20"
          ],
          "pool": "web_pool"
        },
        "web_pool": {
          "class": "Pool",
          "monitors": [
            "http"
          ],
          "members": [
             {
            "servicePort": 80,
            "shareNodes": true,
            "serverAddresses": [
              "192.0.2.10",
              "192.0.2.11"
            ]
          }
             ]
        }
      }
    }
}
